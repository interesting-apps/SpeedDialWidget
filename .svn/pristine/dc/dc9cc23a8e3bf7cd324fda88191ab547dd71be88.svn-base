<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- focused -->
    <item android:state_focused="true" android:drawable="@color/SpeedDialBackground" />
    <!-- focused and pressed-->
    <item android:state_focused="true" android:state_pressed="true" android:drawable="@color/SpeedDialBackground" />
    <!-- pressed -->
    <item android:state_pressed="true" android:drawable="@color/SpeedDialBackground" />
    <!-- default -->
    <item android:drawable="@android:color/transparent" /> 
</selector>